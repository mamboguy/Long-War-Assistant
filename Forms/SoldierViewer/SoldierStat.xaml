             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Long_War_Assistant.Forms.SoldierViewer"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="60">
    <Grid>
        <TabPanel>
            <Image Width="20" Height="20" HorizontalAlignment="Left"/>
            <Label Style="{StaticResource XcomText}" FontSize="20" Padding="0" Content="20" Width="30" Margin="5 -1 0 0" />
        </TabPanel>
    </Grid>
</UserControl>
